<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gdl 3.0 -->
  <!-- interface-requires gtk+ 3.10 -->
  <template class="ValaCATUIFileListWidget" parent="GtkBox">
    <property name="visible">True</property>
    <child>
    <object class="GtkScrolledWindow" id="scrolled_window_file_list">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="shadow_type">in</property>
        <child>
        	<object class="GtkListBox" id="file_list_box">
            <property name="visible">True</property>
            <property name="activate-on-single-click">False</property>
            <signal name="row_activated" handler="on_row_activated" swapped="no"/>
      		</object>
        </child>
      </object>
      <packing>
        <property name="expand">True</property>
        <property name="fill">True</property>
        <property name="position">0</property>
      </packing>
    </child>
  </template>
</interface>