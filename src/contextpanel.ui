<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <!-- interface-requires gtk+ 3.6 -->
  <template class="ValaCATUIContextPanel" parent="GdlDockItem">
    <property name="name" translatable="yes">Context Panel</property>
    <property name="preferred_width">75</property>
    <child>
      <object class="GtkTextView" id="context_textview">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="editable">False</property>
        <property name="left_margin">5</property>
      </object>
    </child>
  </template>
</interface>
