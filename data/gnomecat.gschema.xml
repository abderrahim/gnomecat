<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/info/aquelando/gnomecat/editor/" id="info.aquelando.gnomecat.Editor">
    <key name="highlight" type="b">
      <default>false</default>
      <summary>Highlight syntax</summary>
      <description></description>
    </key>
    <key name="visible-whitespace" type="b">
      <default>true</default>
      <summary>Make whitespace visible</summary>
      <description></description>
    </key>
    <key name="custom-font" type="b">
      <default>true</default>
      <summary>Use custom font.</summary>
      <description></description>
    </key>
    <key name="font" type="s">
      <default>'Monospace 12'</default>
      <summary>Font</summary>
      <description>The font to be used for content.</description>
    </key>
    <key name="message-changed-state" type="s">
      <choices>
        <choice value='fuzzy'/>
        <choice value='translated'/>
      </choices>
      <default>'fuzzy'</default>
      <summary>Message Changed State</summary>
      <description>The state to set when an untranslated message is translated.</description>
    </key>
  </schema>
  <schema id="info.aquelando.gnomecat.ProfilesList"
          path="/info/aquelando/gnomecat/profiles:/">
    <key name="list" type="as">
      <default>[]</default>
    </key>
    <key name="default" type="s">
      <default>""</default>
    </key>
  </schema>
  <schema id="info.aquelando.gnomecat.Profile">
      <key name="uuid" type="s">
        <default>""</default>
      </key>
      <key name="profile-name" type="s">
        <default>""</default>
      </key>
      <key name="translator-name" type="s">
        <default>""</default>
      </key>
      <key name="translator-email" type="s">
        <default>""</default>
      </key>
      <key name="language" type="s">
        <default>""</default>
      </key>
      <key name="plural-form" type="i">
        <default>0</default>
      </key>
      <key name="char-set" type="s">
        <default>""</default>
      </key>
      <key name="encoding" type="s">
        <default>""</default>
      </key>
      <key name="team-email" type="s">
        <default>""</default>
      </key>
  </schema>
</schemalist>